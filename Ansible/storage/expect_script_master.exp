#!/usr/bin/expect

set timeout 180
spawn ./master.sh

expect "exists"
send "y\n"

expect "ENTER"
send "\r\n"

set continue_prompt "Do you want to continue"
expect {
    "$continue_prompt" {
        send "y\n"
    }
    timeout {
        # Nếu không xuất hiện, bỏ qua trường hợp này
        puts "Pass"
    }
}


